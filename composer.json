{
    "name": "eelcol/laravel-country-iso",
    "description": "Convert country ISO codes to full country name - for laravel",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Eelco Luurtsema",
            "email": "mail@eelcoluurtsema.nl"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Eelcol\\LaravelCountryIso\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eelcol\\LaravelCountryIso\\ServiceProvider"
            ]
         }
     },
    "require-dev": {
        "laravel/pint": "^1.18"
    }
}
